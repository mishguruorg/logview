{
  "compilerOptions": {
    "declaration": true,
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es2018",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "lib": [
      "dom",
      "es2017",
      "esnext.asynciterable"
    ],
    "paths": {
      "*": [
        "node_modules/*",
        "src/types/*"
      ],
      "@mishguru/mishmeta": ["src/types/@mishguru-mishmeta.d.ts"],
      "@mishguru/commandquery": ["src/types/@mishguru-commandquery.d.ts"]
    }
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
  ]
}
